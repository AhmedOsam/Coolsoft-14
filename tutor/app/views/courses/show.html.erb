
<h1>Courses#show</h1>

<p>Find me in app/views/courses/show.html.erb</p>


<h1>Dooood</h1>

<%= @course.name %>

<h1>Your Courses</h1>
 
<table border="3">
  <tr>
    <th>Name</th>
    <th>Code</th>
    <th>Year</th>
    <th>Semester</th>
    <th>Description</th>
    <th>Created at</th>
    <th>Updated at</th>
    
  </tr>
 
<% @courses.each do |c| %>
  <tr>
    <td><%= c.name %></td>
    <td><%= c.code %></td>
    <td><%= c.year %></td>
    <td><%= c.semester %></td>
    <td><%= c.description %></td>
    <td><%= c.created_at %></td>
    <td><%= c.updated_at %></td>
    <td><%= button_to "Manage Course" %></td>

  </tr>
  <% end %>
</table>

